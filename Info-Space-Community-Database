SELECT *
FROM [dbo].[INFOSPACE_MOD_DATA]

SELECT *
FROM [dbo].[INFOSPACE_MEMBERS_DATA]


-- A QUERY TO EXTRACT ALL FEMALES FROM [INFOSPACE_MEMBERS_DATA]


SELECT *
FROM [dbo].[INFOSPACE_MEMBERS_DATA]
WHERE GENDER = 'FEMALE'


--A QUERY TO SELECT MEMBERS WHO PREFER TWITTER AND WHOSE EARNINGS ARE ABOVE $500

SELECT [MEMBERS_ID],
		[DISCORD_ID],
		[BIGGEST_WIN$],
		[PREFERRED_SOCIAL_PLATFORM]
FROM [dbo].[INFOSPACE_MEMBERS_DATA]
WHERE [PREFERRED_SOCIAL_PLATFORM] = 'TWITTER'
AND [BIGGEST_WIN$] >500

-- A QUERY TO CATEGORIZE MEMBERS BASE ON AGE GROUP
--19-26 = GENZ
--26-40 = MILLENIAL

SELECT [MEMBERS_ID],
		[DISCORD_ID],
		[AGE],
		[BIGGEST_WIN$],
	CASE 
	WHEN [AGE] BETWEEN '19' AND '26' THEN 'GENZ'
	WHEN [AGE] BETWEEN '26' AND '40' THEN 'MILLENIAL'
	END AS AGE_CATEGORY
FROM [dbo].[INFOSPACE_MEMBERS_DATA]
