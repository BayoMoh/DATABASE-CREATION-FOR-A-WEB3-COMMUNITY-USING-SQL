CREATE DATABASE INFOSPACE_DAO_MEMBER_DETAILS;

USE INFOSPACE_DAO_MEMBER_DETAILS;

CREATE TABLE INFOSPACE_MEMBERS_DATA( MEMBERS_ID INT PRIMARY KEY,
							DISCORD_ID VARCHAR(20),
							AGE INT,
							GENDER VARCHAR(10),
							YEARS_IN_WEB3 INT,
							MARITAL_STATUS VARCHAR(10),
							BIGGEST_WIN$ INT,
							PREFERRED_SOCIAL_PLATFORM VARCHAR(10),
							ROLE_IN_THE_COMMUNITY VARCHAR(10),
							ATTENDS_AMA VARCHAR(10)
							); 

CREATE TABLE INFOSPACE_MOD_DATA(MEMBER_ID INT PRIMARY KEY,
							DISCORD_ID VARCHAR(20),
							AGE INT,
							GENDER VARCHAR(10),
							YEARS_IN_WEB3 INT,
							MARITAL_STATUS VARCHAR(10),
							BIGGEST_WIN$ INT,
							PREFERRED_SOCIAL_PLATFORM VARCHAR(10),
							ROLE_IN_THE_COMMUNITY VARCHAR(10),
							ATTENDS_AMA VARCHAR(10)
							);	

